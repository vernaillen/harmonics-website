{
  "name": "harmonics",
  "version": "1.1.2",
  "scripts": {
    "prebuild": "[[ $CI = true ]] && npx pnpm@3 install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "serve": "pnpm run build && serve dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --fix",
    "netlify:build": "netlify build",
    "netlify:test": "netlify deploy",
    "netlify:deploy": "netlify deploy --prod",
    "netlify:trigger": "netlify deploy --prod --trigger",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:5173 'cypress open --config baseUrl=http://localhost:5173'",
    "cy:local": "start-server-and-test serve http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cy:local"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "animate.css": "^4.1.1",
    "axios": "^1.1.3",
    "pinia": "^2.0.24",
    "prism-theme-vars": "^0.2.4",
    "vee-validate": "^4.7.3",
    "vue": "^3.2.45",
    "vue-easy-lightbox": "^1.10.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/typography": "^0.5.8",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "3.2.0",
    "@vitest/coverage-c8": "^0.25.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.5.0",
    "@vueuse/head": "^1.0.17",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cypress": "^11.1.0",
    "dayjs": "^1.11.6",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.7.0",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^7.7.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "netlify-cli": "^12.2.4",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.4",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-pages-sitemap": "^1.4.0",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vite-plugin-windicss": "^1.8.8",
    "vite-ssg": "^0.22.0",
    "vitest": "^0.25.2",
    "vue-tsc": "^1.0.9"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
