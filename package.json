{
  "name": "harmonics",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --open",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "nuxt generate && serve dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test serve http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i --shamefully-hoist && pnpm run coverage && pnpm run cy:local"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@pinia/nuxt": "^0.4.6",
    "@vueuse/nuxt": "^9.11.1",
    "animate.css": "^4.1.1",
    "vee-validate": "^4.7.3",
    "vue-easy-lightbox": "^1.11.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@nuxt/content": "^2.3.0",
    "@nuxt/image-edge": "1.0.0-27907872.f4dd19d",
    "@nuxt/test-utils": "^3.1.0",
    "@nuxtjs/google-fonts": "3.0.0-1",
    "@nuxtjs/i18n": "8.0.0-beta.7",
    "@nuxtjs/plausible": "^0.2.0",
    "@nuxtjs/tailwindcss": "^6.2.0",
    "@vitest/coverage-c8": "^0.28.1",
    "cypress": "^12.3.0",
    "dayjs": "^1.11.7",
    "jsdom": "^21.1.0",
    "nuxt": "^3.1.0",
    "nuxt-icon": "^0.2.7",
    "remark-breaks": "^3.0.2",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^1.15.3",
    "vitest": "^0.28.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
