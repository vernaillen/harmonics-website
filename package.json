{
  "name": "harmonics",
  "version": "2.4.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "nuxt build && NODE_ENV=production node .output/server/index.mjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test serve http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "lint": "eslint --ext .ts,.vue .",
    "lint:fix": "eslint --ext .ts,.vue . --fix",
    "fulltest": "rm -rf node_modules && pnpm i --shamefully-hoist && pnpm lint && pnpm run coverage && pnpm run cy:local"
  },
  "dependencies": {
    "@mollie/api-client": "^3.7.0",
    "@nuxtjs/axios": "^5.13.6",
    "@pinia/nuxt": "^0.4.9",
    "@sendgrid/mail": "^7.7.0",
    "@upstash/redis": "^1.20.6",
    "@vernaillen/nuxt-base-layer": "^0.1.26",
    "@vueuse/core": "^10.1.2",
    "animate.css": "^4.1.1",
    "vee-validate": "^4.8.6",
    "vue-easy-lightbox": "^1.16.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@nuxt/content": "^2.6.0",
    "@nuxt/devtools": "^0.5.0",
    "@nuxt/image-edge": "1.0.0-28059208.2abef1b",
    "@nuxt/test-utils": "^3.4.2",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "3.0.1",
    "@nuxtjs/i18n": "8.0.0-beta.12",
    "@nuxtjs/plausible": "^0.2.0",
    "@nuxtjs/supabase": "0.3.5",
    "@nuxtjs/tailwindcss": "^6.6.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitest/coverage-c8": "^0.31.0",
    "@vueuse/nuxt": "^10.1.2",
    "consola": "^3.1.0",
    "cypress": "^12.10.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.40.0",
    "jsdom": "^22.0.0",
    "nuxt": "3.5.1",
    "nuxt-icon": "^0.4.0",
    "remark-breaks": "^3.0.2",
