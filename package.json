{
  "name": "harmonics",
  "version": "1.1.1",
  "scripts": {
    "prebuild": "[[ $CI = true ]] && npx pnpm@3 install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "serve": "pnpm run build && serve dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --fix",
    "prepushtest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run build && serve dist",
    "netlify:build": "netlify build",
    "netlify:test": "netlify deploy",
    "netlify:deploy": "netlify deploy --prod",
    "netlify:trigger": "netlify deploy --prod --trigger",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "animate.css": "^4.1.1",
    "pinia": "^2.0.22",
    "prism-theme-vars": "^0.2.4",
    "vee-validate": "^4.6.9",
    "vue": "^3.2.40",
    "vue-easy-lightbox": "^1.8.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.3.0",
    "@vueuse/head": "^0.9.0",
    "c8": "^7.12.0",
    "cypress": "^10.9.0",
    "dayjs": "^1.11.5",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^6.0.4",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "netlify-cli": "^12.0.0",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.4",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-pages-sitemap": "^1.4.0",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vite-plugin-windicss": "^1.8.8",
    "vite-ssg": "^0.20.2",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13",
    "windicss": "^3.5.6"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
