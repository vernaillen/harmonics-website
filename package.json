{
  "name": "harmonics",
  "version": "0.0.2",
  "scripts": {
    "prebuild": "[[ $CI = true ]] && npx pnpm@3 install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "dev": "vite --host",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepushtest": "pnpm run lint && pnpm run coverage && pnpm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.38",
    "@types/tailwindcss": "^3.0.10",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^0.7.6",
    "c8": "^7.11.3",
    "dayjs": "^1.11.2",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^8.7.1",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-md": "^0.13.1",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-pages-sitemap": "^1.2.2",
    "vite-plugin-windicss": "^1.8.4",
    "vite-ssg": "^0.20.0",
    "vitest": "^0.13.0",
    "vue-tsc": "^0.35.0",
    "windicss": "^3.5.4"
  }
}
