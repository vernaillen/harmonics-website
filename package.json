{
  "name": "harmonics",
  "version": "2.4.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "nuxt build && NODE_ENV=production node .output/server/index.mjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test serve http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "lint": "eslint --ext .ts,.vue .",
    "lint:fix": "eslint --ext .ts,.vue . --fix",
    "fulltest": "rm -rf node_modules && pnpm i --shamefully-hoist && pnpm lint && pnpm run coverage && pnpm run cy:local"
  },
  "dependencies": {
    "@mollie/api-client": "^3.7.0",
    "@nuxtjs/axios": "^5.13.6",
    "@pinia/nuxt": "^0.4.11",
    "@sendgrid/mail": "^7.7.0",
    "@upstash/redis": "^1.22.0",
    "@vernaillen/nuxt-base-layer": "^0.2.15",
    "@vueuse/core": "^10.3.0",
    "animate.css": "^4.1.1",
    "vee-validate": "^4.11.1",
    "vue-easy-lightbox": "^1.16.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@nuxt/content": "^2.7.2",
    "@nuxt/devtools": "^0.7.5",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxt/test-utils": "^3.6.5",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "3.0.2",
    "@nuxtjs/i18n": "8.0.0-rc.2",
    "@nuxtjs/plausible": "^0.2.1",
    "@nuxtjs/supabase": "1.0.2",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitest/coverage-v8": "^0.34.1",
    "@vueuse/nuxt": "^10.3.0",
    "consola": "^3.2.3",
    "cypress": "^12.17.3",
    "dayjs": "^1.11.9",
    "eslint": "^8.46.0",
    "happy-dom": "^10.8.0",
    "jsdom": "^22.1.0",
    "nuxt": "3.6.5",
    "nuxt-icon": "^0.4.2",
    "nuxt-vitest": "^0.10.2",
    "remark-breaks": "^3.0.3",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
