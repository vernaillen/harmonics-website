{
  "name": "harmonics",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "nuxt generate && serve dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test serve http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run coverage && pnpm run cy:local"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@pinia/nuxt": "^0.4.6",
    "@vueuse/nuxt": "^9.6.0",
    "animate.css": "^4.1.1",
    "nuxt-lazy-hydrate": "^0.1.0",
    "vee-validate": "^4.7.3",
    "vue-easy-lightbox": "^1.11.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@nuxt/content": "^2.2.2",
    "@nuxt/image-edge": "1.0.0-27809804.194d2d6",
    "@nuxt/test-utils-edge": "3.0.1-rc.0-27821553.ab125bd",
    "@nuxtjs/google-fonts": "^3.0.0-1",
    "@nuxtjs/i18n": "8.0.0-beta.6",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@vitest/coverage-c8": "^0.25.3",
    "cypress": "^11.2.0",
    "dayjs": "^1.11.6",
    "jsdom": "^20.0.3",
    "nuxt": "^3.0.0",
    "nuxt-icon": "^0.1.7",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^1.14.0",
    "vitest": "^0.25.3"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
