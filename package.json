{
  "name": "harmonics",
  "version": "2.4.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "nuxt build && NODE_ENV=production node .output/server/index.mjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test serve http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "lint": "eslint --ext .ts,.vue .",
    "lint:fix": "eslint --ext .ts,.vue . --fix",
    "fulltest": "rm -rf node_modules && pnpm i --shamefully-hoist && pnpm lint && pnpm run coverage && pnpm run cy:local"
  },
  "dependencies": {
    "@mollie/api-client": "^3.7.0",
    "@nuxtjs/axios": "^5.13.6",
    "@pinia/nuxt": "^0.4.8",
    "@sendgrid/mail": "^7.7.0",
    "@vueuse/core": "^10.0.0",
    "animate.css": "^4.1.1",
    "simple-git": "^3.17.0",
    "vee-validate": "^4.8.4",
    "vue-easy-lightbox": "^1.15.0",
    "yup": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@nuxt/content": "^2.5.2",
    "@nuxt/devtools": "^0.4.0",
    "@nuxt/image-edge": "1.0.0-28020728.5df24eb",
    "@nuxt/test-utils": "^3.4.1",
    "@nuxtjs/google-fonts": "3.0.0",
    "@nuxtjs/i18n": "8.0.0-beta.11",
    "@nuxtjs/plausible": "^0.2.0",
    "@nuxtjs/supabase": "0.3.5",
    "@nuxtjs/tailwindcss": "^6.6.6",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitest/coverage-c8": "^0.30.0",
    "@vueuse/nuxt": "^9.13.0",
    "consola": "^3.0.2",
    "cypress": "^12.9.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.38.0",
    "jsdom": "^21.1.1",
    "nuxt": "3.4.1",
    "nuxt-icon": "^0.3.3",
    "remark-breaks": "^3.0.2",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
